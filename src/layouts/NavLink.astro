---
interface Props {
  label: string;
  to: string;
}

const { label, to } = Astro.props;
---

<li>
  <a
    href={to}
    class="no-underline uppercase font-bold text-neutral-500 hover:text-neutral-800 dark:text-neutral-200 dark:hover:text-white transition-colors duration-200 ease-in-out"
  >
    {label}
  </a>
</li>
